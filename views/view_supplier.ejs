<%- include('header') %>
    <!-- partial:partials/_navbar -->
    
    <%- include('navbar') %>
    
    
    
    <!-- partial -->
    <div class="container-fluid page-body-wrapper">
      <!-- partial:partials/_sidebar -->
      <% if(role=="Admin") { %>
        <%- include('Admin_sidebar') %>
        <% } else { %>
          <%- include('sidebar') %>
          <% } %>
      <!-- partial -->
      <div class="main-panel">        
        <div class="content-wrapper">
          <div class="row">
            <div class="col-md-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">View Supplier Details</h4>
                  <p class="card-description">
                      <% if(message.length > 0) { %>
                      <%= message %>
          <% } %></h6>
                  </p>
                  <form class="forms" method="POST" action="view_supplier" >
                    
                    <div class="form-group row">
                      <div class="col-md-6 row"> 
                      <label for="supplierId" class="col-sm-3 col-form-label">Supplier Id:</label>
                      <div class="col-sm-9">
                        <input type="text" maxlength="10" name ="supId"  class="form-control" id="supplierId" placeholder="supplier Id">
                      </div>
                      </div>
                      <div class="col-md-6 row"> 
                        <div class="col-sm-12">
                        <button type="submit" class="col-sm-5  btn btn-sm btn-primary mr-2">Submit</button>
                        </div>  </div>
                        <div class="col-md-6 row"> 
                            <label for="date" class="col-sm-3 col-form-label">Date:</label>
                            <div class="col-sm-9">
                              <label name="date" value="" class="form-control" id="date" placeholder="Date of Registration">     
                                <% if(supplierDetails !== null) { %>
                                  <%= supplierDetails.Date %>
                      <% } %>
                    </label>
                            </div>
                            </div>
                            <div class="col-md-6 row"> 
                              <label for="companyName" class="col-sm-3 col-form-label">Company</label>
                              <div class="col-sm-9">
                                <label name="companyName" class="form-control" id="companyName" placeholder="Name of Organization">     
                                  <% if(supplierDetails !== null) { %>
                                    <%= supplierDetails.SupplierCompanyName %>
                        <% } %>
                      </label>
                              </div>
                              </div>
                          <div class="col-md-6 row"> 
                            <label for="companyEmail" class="col-sm-3 col-form-label">Email</label>
                            <div class="col-sm-9">
                              <label name="companyEmail" class="form-control" id="companyEmail" placeholder="Company's Email">     <% if(supplierDetails !== null) { %>
                                <%= supplierDetails.SupplierCompanyEmail %>
                    <% } %></label>
                            </div>
                        </div>
                        <div class="col-md-12 row"> 
                          <p class=" card-description">
                          Contact Person
                        </p>
                        
                      </div>
                        
                      <div class="col-md-6 row"> 
                        <label for="lastName" class="col-sm-3 col-form-label">Last Name</label>
                        <div class="col-sm-9">
                          <label name="lastName" class="form-control" id="lastName" placeholder="Last Name">     
                            <% if(supplierDetails !== null) { %>
                            <%= supplierDetails.supplierLastName %>
                <% } %></label>
                        </div>
                        </div>
                        <div class="col-md-6 row"> 
                          <label for="firstName" class="col-sm-3 col-form-label">First Name</label>
                          <div class="col-sm-9">
                            <label name="firstName" class="form-control" id="firstName" placeholder="First Name">    <% if(supplierDetails !== null) { %>
                              <%= supplierDetails.SupplierFirstName %>
                  <% } %></label>
                          </div>
                          </div>
                          <div class="col-md-6 row"> 
                            <label for="otherName" class="col-sm-3 col-form-label">Other Name</label>
                            <div class="col-sm-9">
                              <label  name ="otherName" class="form-control" id="otherName" placeholder="Other Name">     <% if(supplierDetails !== null) { %>
                                <%= supplierDetails.SupplierOtherName %>
                    <% } %></label>
                            </div>
                            </div>
                            <div class="col-md-6 row"> 
                              
                              </div>
                              <div class="col-md-6 row"> 
                                <label for="houseNo" class="col-sm-3 col-form-label">House No</label>
                                <div class="col-sm-9">
                                  <label name="houseNo" class="form-control" id="houseNo" placeholder="House Number">     <% if(supplierDetails !== null) { %>
                                    <%= supplierDetails.HouseNo %>
                        <% } %></label>
                                </div>
                                </div>
                                <div class="col-md-6 row"> 
                                  <label for="city" class="col-sm-3 col-form-label">City</label>
                                  <div class="col-sm-9">
                                    <label name="city" class="form-control" id="city" placeholder="City of Residence">     <% if(supplierDetails !== null) { %>
                                      <%= supplierDetails.City %>
                          <% } %></label>
                                  </div>
                                  </div>
                                  <div class="col-md-6 form-group row"> 
                                    <label for="street" class="col-sm-3 col-form-label">Street</label>
                                    <div class="col-sm-9">
                                      <label name="street" class="form-control" id="street" >     <% if(supplierDetails !== null) { %>
                                        <%= supplierDetails.Street %>
                            <% } %></label>
                                      
                                    </div>
                                    </div>
                                    <div class="col-md-6 row"> 
                                      <label for="state"  class="col-sm-3 col-form-label">State</label>
                                      <div class="col-sm-9">
                                        <label name="state" class="form-control" id="state" placeholder="State of Residence">     <% if(supplierDetails !== null) { %>
                                          <%= supplierDetails.State %>
                              <% } %></label>
                                      </div>
                                      </div>
                                      <div class="col-md-6 row"> 
                                        <label for="country" class="col-sm-3 col-form-label">Country</label>
                                        <div class="col-sm-9">
                                          <label name="country" class="form-control" id="country" placeholder="Country of residence">     <% if(supplierDetails !== null) { %>
                                            <%= supplierDetails.Country %>
                                <% } %></label>
                                        </div>
                                        </div>
                        
                     
                        </div>
                      
                  
                  <!--<button type="submit" class="btn btn-primary mr-2">Submit</button>
                    <button class="btn btn-light">Cancel</button> -->  
                  </form>
                </div>
              </div>
            </div>
            
          </div>
        </div>
        <!-- content-wrapper ends -->
        <!-- partial:partials/_footer -->
        <%- include('footer') %>
        <!-- partial -->
      </div>
      <!-- main-panel ends -->
    </div>
    <!-- page-body-wrapper ends -->
  </div>
  <!-- container-scroller -->
  <!-- plugins:js -->
  <script src="vendors/base/vendor.bundle.base.js"></script>
  <!-- endinject -->
  <!-- Plugin js for this page-->
  <!-- End plugin js for this page-->
  <!-- inject:js -->
  <script src="js/off-canvas.js"></script>
  <script src="js/hoverable-collapse.js"></script>
  <script src="js/template.js"></script>
  <!-- endinject -->
  <!-- Custom js for this page-->
  <!-- End custom js for this page-->
</body>

</html>
